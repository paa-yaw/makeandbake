<h1>All Users</h1>
<div class="container">
<% @users.each do |user| %>
  <h3><%= link_to "#{user.first_name}, #{user.last_name}", [:admin, user] %> | <%= link_to "Edit", [:edit, :admin, user] %> | <%= link_to "Delete", [:admin, user], method: :delete, data: {confirm: "Are you sure you want to delete this user?" } %> | <%= link_to "create shop", new_admin_user_shop_path(user) %></h3>
<% end %>
	
</div>